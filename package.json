{
  "name": "n8n-nodes-stabilityai",
  "version": "1.0.2",
  "description": "StabilityAI nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cedricziel/n8n-nodes-stabilityai",
  "author": {
    "name": "Cedric Ziel",
    "email": "mail@cedric-ziel.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cedricziel/n8n-nodes-stabilityai.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/StabilityAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/StabilityAI/StabilityAi.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "form-data": "^4.0.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "form-data": "^4.0.1",
    "n8n-workflow": "*"
  }
}
